{
  "name": "fair-launchpad-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:g8": "next dev -p 3001",
    "build": "prisma generate && next build",
    "build:g8": "prisma generate && cp next.config.g8.ts next.config.js && next build && cp next.config.js next.config.backup.js",
    "start": "next start",
    "start:g8": "next start -p 3001",
    "lint": "next lint",
    "lint:g8": "next lint --dir src/app/g8 --dir src/components/g8",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "type-check": "tsc --noEmit",
    "type-check:g8": "tsc --noEmit",
    "test": "jest",
    "test:g8": "jest --testPathPattern=g8",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:g8": "vercel --prod --config vercel.g8.json",
    "deploy:g8:preview": "vercel --config vercel.g8.json"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "^5.7.1",
    "@privy-io/react-auth": "^2.25.0",
    "@privy-io/wagmi": "^1.0.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.17.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "ethers": "^6.8.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.2.0",
    "viem": "^2.21.36",
    "wagmi": "^1.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5"
  }
}
