    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "ethers": "^6.8.1",
    "lucide-react": "^0.303.0",
    "next": "15.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.2.0",
    "viem": "^1.19.9",
    "wagmi": "^2.0.0",
    "zod": "^3.22.4"
