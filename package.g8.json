{
  "name": "g8-platform",
  "version": "2.0.0",
  "description": "G8 - Next-generation token launchpad with World ID integration",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:g8": "next dev -p 3001",
    "build": "next build",
    "build:g8": "next build --config next.config.g8.ts",
    "start": "next start",
    "start:g8": "next start -p 3001",
    "lint": "next lint",
    "lint:g8": "next lint --dir src/app/g8 --dir src/components/g8",
    "type-check": "tsc --noEmit",
    "type-check:g8": "tsc --noEmit --project tsconfig.g8.json",
    "test": "jest",
    "test:g8": "jest --testPathPattern=g8",
    "deploy:g8": "vercel --prod --config vercel.g8.json",
    "deploy:g8:preview": "vercel --config vercel.g8.json"
  },
  "dependencies": {
